{
  "name": "lua-httpd_1-1",
  "version": "1.0.0",
  "description": "HTTP/1.1 Server for LUA with no deps",
  "license": "MIT",
  "scripts": {
    "install:windows": "choco install lua",
    "install:osx": "brew install lua",
    "prettier:write": "prettier . -w",
    "prettier": "prettier . -c",
    "clean": "rimraf dist",
    "dev": "cd src/ && nodemon -x lua init.lua",
    "test": "concurrently -k -s last \"npm run dev\" \"jest\"",
    "test:watch": "nodemon --watch './src/**.*' --watch './test/**.test.js' -x npm run test"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@jest/types": "^29.4.1",
    "axios": "^1.2.6",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "statuses": "^2.0.1"
  }
}
